{
  "name": "ezeikel-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json|ts|tsx|html|md)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit ./next-env.d.ts"
    ],
    "**/*.+(js|jsx|json|ts|tsx|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests --passWithNoTests"
    ]
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1",
      "@opentelemetry/core": "1.13.0",
      "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
      "@opentelemetry/resources": "1.13.0",
      "@opentelemetry/sdk-trace-base": "1.13.0",
      "@opentelemetry/sdk-trace-node": "1.13.0",
      "@opentelemetry/semantic-conventions": "1.13.0"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.4.0",
    "@fortawesome/pro-light-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/pro-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@next/mdx": "^13.4.11",
    "@sentry/nextjs": "^7.60.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "formik": "^2.4.2",
    "next": "13.4.11",
    "next-contentlayer": "^0.3.4",
    "next-mdx-remote": "^4.4.1",
    "plaiceholder": "^2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-textarea-autosize": "^8.5.2",
    "react-tweet": "^3.0.3",
    "react-wrap-balancer": "^0.4.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.9.4",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.14.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/mailchimp__mailchimp_marketing": "^3.0.10",
    "@types/mdx": "^2.0.5",
    "@types/node": "18.14.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "13.4.11",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-runner-eslint": "^1.1.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "tsc-files": "^1.1.4",
    "typescript": "4.9.5"
  }
}
