{
  "name": "ezeikel-website",
  "description": "Personal website for Ezeikel",
  "version": "0.1.0",
  "author": "Ezeikel <hi@ezeikel.dev>",
  "license": "ISC",
  "scripts": {
    "dev": "yarn develop",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc --noEmit",
    "check-types:watch": "yarn check-types --watch",
    "lint": "jest --config test/jest.lint.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "build": "yarn check-types && yarn preload-fonts && gatsby build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "preload-fonts": "gatsby-preload-fonts",
    "prepare": "husky install"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|html|md)": [
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.23.0",
    "babel-plugin-styled-components": "^1.13.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "formik": "^2.2.9",
    "gatsby": "^4.0.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^4.0.0",
    "gatsby-plugin-hotjar": "^1.1.1",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-offline": "^5.0.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-preload-fonts": "^3.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^4.0.0",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-plugin-styled-components": "^5.0.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-contentful": "^6.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-remark": "^5.0.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "lodash": "^4.17.21",
    "postmark": "^2.7.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-textarea-autosize": "^8.3.3",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/react-textarea-autosize": "^4.3.6",
    "@types/styled-components": "^5.1.15",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-preset-gatsby": "^2.0.0",
    "cypress": "^8.6.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.3.1",
    "jest-runner-eslint": "^1.0.0",
    "jest-styled-components": "^7.0.5",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^11.2.4",
    "prettier": "^2.4.1",
    "test-data-bot": "^0.8.0",
    "typescript": "^4.4.4"
  }
}
